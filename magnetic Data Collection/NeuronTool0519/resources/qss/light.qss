QToolTip {
    color: #FFFFFF;
    border: 1px solid #323232;
    background: #5A5A5A;
    opacity: 200;
}

/* Status bar */
QStatusBar {
    color: #DCDCDC;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #6E6E6E, stop:1 #464646);
}

QStatusBar::item {
    border: 1px solid #323232;
    border-radius: 2px;
}

/* Group box */
QGroupBox {
    border: 2px solid gray;
    border-radius: 4px;
    margin-top: 2ex;
    background: transparent;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
}

/* Label */
QLabel {
    color: #DCDCDC;
    border-radius: 0px;
    border-style: none;
    background: transparent;
}

/* Line edit, text edit */
QLineEdit, QTextEdit {
    color: #DCDCDC;
    border: 1px solid #323232;
    padding: 2px;
    background: #464646;
    selection-background-color: #3278C8;
}

/* Slider */
QSlider::groove:horizontal {
    height: 8px;
    border: 1px solid #323232;
    background: #B4B4B4;
}

QSlider::handle:horizontal {
    width: 16px;
    border-width: 0px;
    border-style: none;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #6E6E6E, stop:1 #464646);
}

QSlider::handle:hover {
    background: #787878;
}

QSlider::handle:pressed {
    background: #3C3C3C;
}

/* Push button */
QPushButton {
    color: #DCDCDC;
    border-width: 0px;
    border-style: none;
    border-radius: 4px;
    padding: 4px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #505050, stop:1 #3C3C3C);
}

QPushButton:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #8C8C8C, stop:1 #787878);
}

QPushButton:pressed {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #3C3C3C, stop:1 #282828);
}

/* Check box */
QCheckBox {
    color: #DCDCDC;
    border-radius: 4px;
    spacing: 4px;
    padding: 2px;
    background: transparent;
}

/*QCheckBox::indicator:unchecked {
    image: url(:/image/checkbox_unchecked.png);
}

QCheckBox::indicator:checked {
    image: url(:/image/checkbox_checked.png);
}*/

/* Radio button */
QRadioButton {
    color: #DCDCDC;
    border-radius: 4px;
    spacing: 4px;
    padding: 2px;
    background: transparent;
}

/*
QRadioButton::indicator::unchecked {
    image: url(:/image/radio_normal.png);
}

QRadioButton::indicator::checked {
    image: url(:/image/radio_selected.png);
}*/

/* Combo box */
QComboBox {
    color: #DCDCDC;
    border: 1px solid #323232;
    padding: 2px;
    background: #464646;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 16px;
    border-left-width: 1px;
    border-left-style: solid;
    border-left-color: #323232;
}

QComboBox::down-arrow {
    image: url(:/qss/resources/qss/float.ico);
}

QListView, QTreeView {
    color: #DCDCDC;
    background: #464646;
}

QListView::item:hover, QTreeView::item:hover {
    background: #3C3C3C;
}

QListView::item:selected, QTreeView::item:selected {
    color: #FFFFFF;
    background: #3278C8;
}

QHeaderView::section {
    color: #DCDCDC;
    border-width: 0px;
    border-style: none;
    border-right: 1px solid #3C3C3C;
    border-bottom: 1px solid #3C3C3C;
    padding: 2px;
    padding-left: 4px;
    background: #505050;
}

QHeaderView::section:checked
{
    background: #3278C8;
}

/*QHeaderView::down-arrow {
    image: url(down_arrow.png);
}

QHeaderView::up-arrow {
    image: url(up_arrow.png);
}*/

/* Hover */
QRadioButton:hover, QCheckBox:hover, QComboBox::hover {
    background: #787878;
}

/* Pressed */
QRadioButton:pressed, QCheckBox:pressed, QComboBox::pressed {
    background: #3C3C3C;
}

/* Focus */
QLineEdit:focus, QFrame:focus {
    border-width: 2px;
}

/* Scroll bar */
QScrollBar:horizontal {
    height: 16px;
    margin: 0px 18px 0 18px;
    border: 1px solid #323232;
    background: #464646;
}

QScrollBar::handle:horizontal {
    min-height: 16px;
    background: #787878;
}

QScrollBar::add-line:horizontal {
    width: 16px;
    subcontrol-position: right;
    subcontrol-origin: margin;
    border: 1px solid #323232;
    background: #5A5A5A;
}

QScrollBar::sub-line:horizontal {
    width: 16px;
    subcontrol-position: left;
    subcontrol-origin: margin;
    border: 1px solid #323232;
    background: #5A5A5A;
}

QScrollBar:left-arrow:horizontal, QScrollBar::right-arrow:horizontal {
    width: 4px;
    height: 4px;
    border: 1px solid #323232;
    background: #464646;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}

QScrollBar:vertical {
    width: 16px;
    margin: 18px 0 18px 0;
    border: 1px solid #323232;
    background: #464646;
}

QScrollBar::handle:vertical {
    min-height: 16px;
    background: #787878;
}

QScrollBar::add-line:vertical {
    height: 16px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
    border: 1px solid #323232;
    background: #5A5A5A;
}

QScrollBar::sub-line:vertical {
    height: 16px;
    subcontrol-position: top;
    subcontrol-origin: margin;
    border: 1px solid #323232;
    background: #5A5A5A;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
    width: 4px;
    height: 4px;
    border: 1px solid #323232;
    background: #464646;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}
