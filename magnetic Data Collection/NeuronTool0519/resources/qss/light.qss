/* Widget */
QWidget {
    color: #DCDCDC;
    background: #5A5A5A;
}

QWidget::disabled {
    color: gray;
    font: italic;
}

/* Dock widget */
QDockWidget {
    color: #F0F0F0;
    font: bold;
    titlebar-close-icon: url(:/qss/resources/qss/close.png);
    titlebar-normal-icon: url(:/qss/resources/qss/float.png);
}

QDockWidget::title {
    text-align: left;
    border-width: 0px;
    border-style: none;
    padding-left: 4px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop: 0   #646464, stop: 0.5 #505050,
                                stop: 0.6 #464646, stop: 1   #6E6E6E);

}

QDockWidget::close-button, QDockWidget::float-button {
    border: 1px solid transparent;
    padding: 0px;
    background-color: transparent;
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background: #787878;
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
    background: #3C3C3C;
}

/* Separator */
QMainWindow::separator {
    width: 2px;
    height: 2px;
    background: #3C3C3C;
}

/* Tab widget */
QTabBar::tab {
    color: #DCDCDC;
    border-right: 1px solid #323232;
    padding: 6px;
    background: #464646;
}

QTabBar::tab:hover {
    background: #787878;
}

QTabBar::tab:selected {
    color: #FFFFFF;
    /*background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #787878, stop:1 #646464);*/
    background: #5A5A5A;
}

/* Tool box */
QToolBox::tab {
    color: #DCDCDC;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #787878, stop:1 #646464);
}

QToolBox::tab:hover {
    color: #FFFFFF;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #8C8C8C, stop:1 #787878);
}

QToolBox::tab:selected {
    color: #FFFFFF;
    font: bold;
}

/* Menu */
QMenu {
    color: #DCDCDC;
    border: 1px solid #323232;
}

QMenu::item {
    background-color: transparent;
}

QMenu::item:selected {
    color: #FFFFFF;
    background: #3C3C3C;
}

QMenu::separator {
    height: 1px;
    background: #323232;
}

/* Menu bar */
QMenuBar {
    color: #DCDCDC;
    border-bottom: 1px solid #323232;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #3C3C3C, stop:1 #5A5A5A);
}

QMenuBar::item {
    padding: 0px 8px;
    background: transparent;
}

QMenuBar::item:selected {
    background: #787878;
}

QMenuBar::item:pressed {
    background: #3C3C3C;
}

/* Tool bar */
QToolBar
{
    color: #DCDCDC;
    /*border: 1px solid #323232;*/
}

QToolBar::handle {
    image: none;
}

QToolBar::separator
{
    width: 2px;
    margin: 4px 0px;
    background: #323232;
}

QToolButton {
    color: #DCDCDC;
    border-width: 0px;
    border-style: none;
    border-radius: 4px;
}

QToolButton:hover {
    background: #787878;
}

QToolButton:pressed {
    background: #3C3C3C;
}

QToolButton:checked {
    background: #3C3C3C;
}

